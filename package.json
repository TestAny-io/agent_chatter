{
  "name": "@testany/agent-chatter",
  "description": "Multi-agent conversation orchestration CLI for AI assistants",
  "version": "0.2.1",
  "type": "module",
  "main": "./out/cli.js",
  "bin": {
    "agent-chatter": "./out/cli.js"
  },
  "scripts": {
    "compile": "tsc -p ./",
    "build": "tsc",
    "watch": "tsc -watch -p ./",
    "test": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:watch": "vitest watch",
    "prepublishOnly": "npm run build && npm test",
    "pack:local": "npm pack",
    "install:global": "npm install -g $(npm pack | tail -1)",
    "uninstall:global": "npm uninstall -g @testany/agent-chatter",
    "link:dev": "npm run build && npm link",
    "unlink:dev": "npm unlink -g"
  },
  "files": [
    "out/**/*",
    "schemas/**/*",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "cli",
    "ai",
    "agent",
    "conversation",
    "orchestration",
    "claude",
    "gemini",
    "openai",
    "multi-agent"
  ],
  "author": "TestAny.io <support@testany.io>",
  "license": "UNLICENSED",
  "private": false,
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^19.2.5",
    "ink-testing-library": "^4.0.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.9"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "ink": "npm:@jrichman/ink@^6.4.3",
    "ink-text-input": "^6.0.0",
    "inquirer": "^9.3.8",
    "inquirer-autocomplete-standalone": "^0.8.1",
    "react": "^19.2.0"
  }
}
