{
  "name": "@testany/agent-chatter-core",
  "version": "1.1.1",
  "description": "Core library for multi-agent conversation orchestration",
  "type": "module",
  "main": "./out/index.js",
  "types": "./out/index.d.ts",
  "exports": {
    ".": {
      "types": "./out/index.d.ts",
      "import": "./out/index.js"
    }
  },
  "license": "(MPL-2.0 OR Apache-2.0)",
  "author": "TestAny.io <support@testany.io>",
  "repository": {
    "type": "git",
    "url": "https://github.com/TestAny-io/agent_chatter.git"
  },
  "keywords": [
    "agent",
    "conversation",
    "orchestration",
    "multi-agent",
    "ai"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "clean": "rm -rf out"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.9"
  },
  "files": [
    "out/**/*",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  }
}
